---
import { ProjectPage } from "@/components/pages";
import { getProjects } from "@/data";
const { props } = Astro;

export function getStaticPaths() {
  const PROJECTS = getProjects();
  return PROJECTS.map((project) => ({
    params: {
      slug: project.slug,
    },
  }));
}
---

<ProjectPage props={props} />
