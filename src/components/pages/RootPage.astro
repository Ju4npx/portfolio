---
import { Layout } from "@/layouts";
import { About, Experience, Footer, Hero, Navbar, Projects } from "@/sections";
import i18next, { t } from "i18next";

i18next.changeLanguage(Astro.currentLocale);
---

<Layout
  title={t("seo.title")}
  description={t("seo.description")}
  tags={t("seo.tags")}
  imageUrl={"https://jpmachado.dev/thumbnail.webp"}
>
  <Navbar />
  <Hero />
  <Experience />
  <Projects />
  <About />
  <Footer />
</Layout>
